<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="500.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="productivityApp.SignUpController">

   <children>
      <TextField fx:id="idField" layoutX="127.0" layoutY="100.0"
                 prefHeight="39.0" prefWidth="241.0" promptText="Student ID"/>
      <TextField fx:id="usernameid" layoutX="127.0" layoutY="150.0"
                 prefHeight="39.0" prefWidth="241.0" promptText="Username"/>
      <TextField fx:id="gradeLevelField" layoutX="127.0" layoutY="200.0"
                 prefHeight="39.0" prefWidth="241.0" promptText="Grade Level"/>
      <DatePicker fx:id="birthday" layoutX="127.0" layoutY="250.0"
                  prefHeight="39.0" prefWidth="241.0"/>
      <PasswordField fx:id="passid" layoutX="127.0" layoutY="300.0"
                     prefHeight="26.0" prefWidth="241.0" promptText="Password"/>
      <PasswordField fx:id="cpassid" layoutX="127.0" layoutY="350.0"
                     prefHeight="26.0" prefWidth="241.0" promptText="Confirm Password"/>
      <Button fx:id="btnRegister" layoutX="127.0" layoutY="400.0"
              prefHeight="46.0" prefWidth="265.0" text="REGISTER"
              onAction="#lblClickRegister"/>
      <Hyperlink fx:id="lblLogin" layoutX="159.0" layoutY="460.0"
                 text="Already have an account? LOGIN"
                 onAction="#lblClickLogin"/>
   </children>
</AnchorPane>

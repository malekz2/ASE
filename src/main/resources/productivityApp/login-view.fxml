<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="500.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="productivityApp.LoginController">

   <children>
      <TextField fx:id="usernameid" layoutX="127.0" layoutY="184.0"
                 prefHeight="26.0" prefWidth="219.0" promptText="Username"/>
      <PasswordField fx:id="passid" layoutX="123.0" layoutY="233.0"
                     prefHeight="26.0" prefWidth="254.0" promptText="Password"/>
      <Button fx:id="btnLogin" layoutX="127.0" layoutY="300.0"
              mnemonicParsing="false" onAction="#lblCklckLogin"
              prefHeight="46.0" prefWidth="265.0" text="LOGIN"/>
      <Hyperlink fx:id="lblSignUp" layoutX="139.0" layoutY="360.0"
                 onAction="#lblClckSignUp"
                 text="Dont have an account yet? SIGN UP"/>
   </children>
</AnchorPane>
